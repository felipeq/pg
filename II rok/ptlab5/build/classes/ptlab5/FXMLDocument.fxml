    <TableView fx:id="tableView" layoutX="14.0" layoutY="14.0" prefHeight="200.0" prefWidth="200.0">
      <columns>
        <TableColumn text="ID" fx:id="bookIDColumn">
          <cellValueFactory>
            <PropertyValueFactory property="id" />
          </cellValueFactory>
        </TableColumn>
        <TableColumn text="Tytuł" fx:id="titleColumn">
          <cellValueFactory>
            <PropertyValueFactory property="title" />
          </cellValueFactory>
        </TableColumn>
      </columns>
    </TableView>
    <TableView  fx:id="authorsTableView" layoutX="236.0" layoutY="14.0" prefHeight="200.0" prefWidth="246.0">
      <columns>
        <TableColumn prefWidth="75.0" text="ID" fx:id="authorIDColumn" />
        <TableColumn prefWidth="75.0" text="Imię" fx:id="nameColumn" />
        <TableColumn prefWidth="75.0" text="Nazwisko" fx:id="surnameColumn" />
      </columns>
    </TableView>
  </children>
