<AnchorPane id="AnchorPane" minHeight="270.0" prefHeight="270.0" prefWidth="563.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="ptlab5.FXMLDocumentController">
